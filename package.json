{
  "name": "ibiza-calendar-sunandflower",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "build:prod": "tsc && vite build --mode production",
    "build:subdomain": "BASE_PATH='/calendar' vite build --mode production",
    "preview": "vite preview",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist && rm -rf node_modules/.vite",
    "scraper": "node --loader ts-node/esm scraper/index.ts",
    "scraper:dev": "nodemon --exec node --loader ts-node/esm scraper/index.ts"
  },
  "dependencies": {
    "@google/genai": "^1.0.1",
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.49.10",
    "axios": "^1.6.0",
    "cheerio": "^1.0.0-rc.12",
    "date-fns": "^2.30.0",
    "dotenv": "^16.3.0",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "mysql2": "^3.14.1",
    "node-cron": "^3.0.3",
    "openai": "^4.20.0",
    "pg": "^8.16.0",
    "puppeteer": "^21.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.2",
    "react-router-dom": "6",
    "string-similarity": "^4.0.4",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.1.8",
    "@types/node": "^22.14.0",
    "@types/node-cron": "^3.0.11",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@types/string-similarity": "^4.0.2",
    "autoprefixer": "^10.4.21",
    "nodemon": "^3.0.0",
    "postcss": "^8.5.4",
    "tailwindcss": "^4.1.8",
    "ts-node": "^10.9.0",
    "typescript": "~5.7.2",
    "vite": "^6.2.0"
  }
}
