{
  "name": "ibiza-calendar-sunandflower",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "tsc && vite build --mode production",
    "build:deploy": "vite build",
    "preview": "vite preview",
    "preview:host": "vite preview --host 0.0.0.0 --port 5177",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist && rm -rf node_modules/.vite",
    "lighthouse": "lhci autorun",
    "lighthouse:local": "lhci autorun --config=./lighthouserc.json",
    "setup-db": "node scripts/setup-database.js",
    "deploy": "npm run build:deploy && npm run setup-db"
  },
  "dependencies": {
    "@heroicons/react": "^2.2.0",
    "@supabase/supabase-js": "^2.49.8",
    "@vercel/analytics": "^1.3.1",
    "@vercel/speed-insights": "^1.1.0",
    "date-fns": "^2.30.0",
    "dotenv": "^16.5.0",
    "i18next": "^23.11.5",
    "i18next-browser-languagedetector": "^8.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^14.1.2",
    "react-router-dom": "6"
  },
  "devDependencies": {
    "@lhci/cli": "^0.14.0",
    "@types/node": "^22.14.0",
    "@types/react": "^19.1.5",
    "@types/react-dom": "^19.1.5",
    "@vitejs/plugin-react": "^4.5.1",
    "autoprefixer": "^10.4.21",
    "lighthouse": "^12.6.1",
    "postcss": "^8.5.4",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.7.2",
    "vite": "^6.2.0"
  }
}
